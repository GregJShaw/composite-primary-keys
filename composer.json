{
  "name": "maksimru/composite-primary-keys",
  "description": "Most advanced composite primary keys package with support of: binary columns, queueable, implicit route binding, relations",
  "keywords": [
    "eloquent",
    "composite",
    "binary",
    "keys"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Martianov",
      "email": "maxmtmn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "illuminate/cache": "^5.8",
    "illuminate/contracts": "^5.8",
    "illuminate/queue": "^5.8",
    "illuminate/redis": "^5.8",
    "illuminate/session": "^5.8",
    "illuminate/support": "^5.8"
  },
  "require-dev": {
    "laravel/framework": "^5.8",
    "orchestra/testbench": "~3.6",
    "orchestra/testbench-browser-kit": "~3.6",
    "orchestra/database": "~3.0",
    "mockery/mockery": "0.9.* || 1.3.*",
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "7",
    "barryvdh/laravel-debugbar": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "MaksimM\\CompositePrimaryKeys\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/database/migrations",
      "tests/database/seeders"
    ],
    "psr-4": {
      "MaksimM\\CompositePrimaryKeys\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
